{
    "name": "my-project",
    "version": "0.0.0",
    "description": "A super cool app/server/tool/library/widget/thingy",
    "license": "MIT",
    "keywords": [],
    "scripts": {
        "build:es": "parcel build --out-dir ./dist --public-url ./ ./assets/index.html",
        "watch:es": "npm run build:es",
        "lint": "eslint . -c ./.eslintrc.js --fix",
        "lint:ing": "watch 'npm run lint' ./src",
        "lint:tests": "eslint __tests__/**/*.js -c ./.eslintrc.js --fix --no-ignore",
        "build:css": "postcss ./assets/css/style.css --dir ./dist",
        "watch:css": "npm run build:css -- --watch",
        "test": "jest .*.test.js --coverage",
        "test:ing": "npm test -- --watchAll",
        "predeploy": "npm-run-all clean build:es build:css copy:assets",
        "deploy": "surge dist",
        "clean": "del-cli ./dist",
        "copy": "npm-run-all --parallel copy:assets copy:index",
        "copy:assets": "cpy './assets/!(css)/**/*.*' './assets/**/[.]*' ./dist --parents --recursive",
        "copy:index": "cpy './assets/index.html' ./dist",
        "prebuild:es": "npm run clean",
        "watch:assets": "watch 'npm run copy' ./assets",
        "prewatch:es": "npm run clean",
        "serve": "parcel ./assets/index.html --out-dir ./dist --port 4669 --open",
        "start": "npm-run-all --parallel watch:assets serve",
        "dev": "stmux [ \"npm run watch:es\" : \"npm run lint:ing\" ]"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime": "^7.5.5",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-preset-minify": "^0.5.1",
        "cpy-cli": "^2.0.0",
        "cssnano": "^4.1.10",
        "del-cli": "^2.0.0",
        "eslint": "^6.3.0",
        "eslint-config-omaha-prime-grade": "^11.0.4",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "jest": "^24.9.0",
        "jest-watch-typeahead": "^0.4.0",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.3",
        "parcel-plugin-purgecss": "^2.1.2",
        "postcss-cli": "^6.1.3",
        "postcss-import": "^12.0.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^6.0.1",
        "postcss-safe-parser": "^4.0.1",
        "react-hot-loader": "^4.12.12",
        "stmux": "^1.7.1",
        "stylelint": "^10.1.0",
        "stylelint-config-recommended": "^2.2.0",
        "watch": "^1.0.2"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.(e2e|test).[jt]s?(x)"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/__tests__/setup.js"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "alias": {
        "react-dom": "@hot-loader/react-dom"
    },
    "main": "./src/main.js",
    "dependencies": {
        "@hot-loader/react-dom": "^16.9.0",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "wouter": "^2.2.1"
    }
}
